CPP = cl.exe
INCLUDES = /I . /I ..\..\..\include
CPP_OPTIONS = /nologo /EHsc /MD /O2 /D _CRT_SECURE_NO_WARNINGS /D YAMI4_DLL_COMPILATION /c

ARCHIVE_OBJS = \
	delay.obj \
	flag.obj \
	semaphore.obj \
	start_thread.obj

all : $(ARCHIVE_OBJS)

.cpp.obj::
	$(CPP) $(CPP_OPTIONS) $(INCLUDES) $< 

clean :
	del /Q *.obj
